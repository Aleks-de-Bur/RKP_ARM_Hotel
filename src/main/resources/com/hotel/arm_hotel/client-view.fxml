<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="300.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hotel.arm_hotel.Controllers.ClientController">
    <children>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Фамилия" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="tfLastName" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" />
                            </VBox.margin>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" text="Имя" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="tfFirstName" layoutX="10.0" layoutY="27.0" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" />
                            </VBox.margin>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" text="Отчество" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="tfPatronymic" layoutX="10.0" layoutY="27.0" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" />
                            </VBox.margin>
                        </TextField>
                  <Label layoutX="20.0" layoutY="124.0" text="Дата рождения">
                     <VBox.margin>
                        <Insets left="10.0" top="10.0" />
                     </VBox.margin>
                  </Label>
                  <DatePicker fx:id="dtpBorn" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
                     <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </VBox.margin>
                  </DatePicker>
                    </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
                </VBox>
                <VBox HBox.hgrow="ALWAYS">
                    <children>
                        <Label text="Серия паспорта" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="tfSeria" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" />
                            </VBox.margin>
                        </TextField>
                        <Label layoutX="10.0" layoutY="10.0" text="Номер паспорта" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" top="10.0" />
                            </VBox.margin>
                        </Label>
                        <TextField fx:id="tfNumber" layoutX="10.0" layoutY="27.0" VBox.vgrow="ALWAYS">
                            <VBox.margin>
                                <Insets left="10.0" right="10.0" />
                            </VBox.margin>
                        </TextField>
                  <Label layoutX="20.0" layoutY="72.0" text="Номер телефона">
                     <VBox.margin>
                        <Insets left="10.0" top="10.0" />
